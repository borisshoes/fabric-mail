{
  "schemaVersion": 1,
  "id": "fabricmail",
  "version": "${version}",
  "name": "Fabric Mail",
  "description": "",
  "authors": [
    "BorisShoes"
  ],
  "contact": {
    "repo": "https://github.com/borisshoes/fabric-mail"
  },
  "license": "OSL-3.0",
  "icon": "assets/fabricmail/icon.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      "net.borisshoes.fabricmail.FabricMail"
    ],
    "cardinal-components-world": [
      "net.borisshoes.fabricmail.cardinalcomponents.WorldDataComponentInitializer"
    ]
  },
  "custom": {
    "cardinal-components": ["fabricmail:mail"]
  },
  "mixins": [
    "fabricmail.mixins.json"
  ],
  "accessWidener" : "fabricmail.accesswidener",
  "depends": {
    "fabricloader": ">=${loader_version}",
    "fabric": "*",
    "minecraft": ">=${minecraft_version}"
  }
}
